{
    "name": "jukuan/img-to-web",
    "type": "library",
    "license": "MIT",
    "description": "Converts images to web-friendly format (or JPG, PNG)",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Jukuan\\ImgToWeb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Juljan M.",
            "email": "y.misiukevich@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "ext-gd": "*",
        "php": ">=7.4"
    },
    "require-dev": {
        "vimeo/psalm": "^5.11",
        "phpunit/phpunit": "^10.1"
    },
    "scripts": {
        "test": "phpunit Tests"
    }
}
